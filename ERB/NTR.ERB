;-------------------------------------------------------------------------------
;NTRカウンター関連
;住人が訪問者に対して軽いセクハラを実行
@NTR_COUNTER(奴隷)
	#DIM 奴隷
	#DIM セクハラ行為
	セクハラ行為 = 0
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ)
		セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		SIF RAND:2
			セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		;主人も同席
		IF CFLAG:MASTER:現在位置 == FLAG:訪問者の現在位置
			;浮気公認レベルで変化
			IF TALENT:奴隷:浮気公認 == 0
				SIF RAND:3
					セクハラ行為 = RAND:(NTR_CNT_股間をまさぐる+1)
			ELSE
				SIF RAND:2
					セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
			ENDIF
			;主人不在
		ELSE
			SIF RAND:2
				セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
		;主人も同席
		IF CFLAG:MASTER:現在位置 == FLAG:訪問者の現在位置
			;浮気公認レベルで変化
			IF TALENT:奴隷:浮気公認 == 0
				SIF RAND:3
					セクハラ行為 = RAND:(NTR_CNT_キス+1)
			ELSEIF TALENT:奴隷:浮気公認 == 1
				SIF RAND:2
					セクハラ行為 = RAND:(NTR_CNT_股間をまさぐる+1)
			ELSE
				SIF RAND:2
					セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
			ENDIF
			;主人不在
		ELSE
			SIF RAND:2
				セクハラ行為 = RAND:(NTR_CNT_ディープキス+1)
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_うふふする程度)
		;主人も同席
		IF CFLAG:MASTER:現在位置 == FLAG:訪問者の現在位置 
			SIF RAND:4
				セクハラ行為 = RAND:(NTR_CNT_耳元に息+1)
			;主人不在
		ELSE
			SIF RAND:3
				セクハラ行為 = RAND:(NTR_CNT_対面抱擁+1)
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_奉仕する程度)
		;主人も同席
		IF CFLAG:MASTER:現在位置 == FLAG:訪問者の現在位置
			SIF RAND:5
				セクハラ行為 = RAND:(NTR_CNT_見つめる+1)
			;主人不在
		ELSE
			SIF RAND:4
				セクハラ行為 = RAND:(NTR_CNT_キス+1)
		ENDIF
	ELSE
		セクハラ行為 = NTR_CNT_なし
	ENDIF
	;寝ていたらカウンターはしない
	IF CFLAG:奴隷:睡眠
		セクハラ行為 = NTR_CNT_なし
	ENDIF
	IF セクハラ行為
		CALL NTR_COUNTER_DO(奴隷, セクハラ行為)
	ENDIF
	;NTRカウンター実行の保存
	CFLAG:奴隷:NTRカウンターセクハラ = セクハラ行為
RETURN 1

