			CASE NTR_行為_Ｖ正常位 TO NTR_行為_Ｖ背面立位
				CALL NTR_MESSAGE_SEX_SHOW(奴隷)
				;アナルセックス
			CASE NTR_行為_Ａ正常位 TO NTR_行為_Ａ背面立位
				CALL NTR_MESSAGE_A_SEX_SHOW(奴隷)
			CASEELSE
				PRINTW バグです
		ENDSELECT
	ENDIF
RETURN 1

