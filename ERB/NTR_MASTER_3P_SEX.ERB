		EXP:加害者V:レズ経験 += 2
	ENDIF
	;V射精処理
	CALL EJACULATION_V(被害者, 加害者V)
	;V感覚が高いと絶頂経験加算（強絶頂扱い）
	;CALL NTR_V_EXP_PLUS(被害者)
	;Ａ側処理
	IF -1 != 加害者A
