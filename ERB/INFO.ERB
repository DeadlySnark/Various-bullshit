;-------------------------------------------------------------------------------
;起床中の基本情報（時間、HPMP、各種パラメータなど）を表示する
@SHOW_STATUS
	#DIM 行動回数
	;てすと
;	PRINTL てすと
;	
;	FOR LOCAL, 0, CHARANUM
;		PRINTFORML %CSTR:LOCAL:フルネーム%%CSTR:LOCAL:セックスした相手%
;		SIF LOCAL==CHARA_ACTIVE
;			PRINTL ---------------
;		FOR LOCAL:1, 0, CHARANUM
;			SIF RELATION:(LOCAL):(NO:(LOCAL:1))!=100&&RELATION:(LOCAL):(NO:(LOCAL:1))!=0
;				PRINTFORML {LOCAL}%CSTR:LOCAL:フルネーム% {RELATION:(LOCAL):(NO:(LOCAL:1))} %CSTR:(LOCAL:1):フルネーム%
;		NEXT
;		PRINTFORM %CALLNAME:COUNT%  
;		PRINTFORML %CSTR:LOCAL:フルネーム% {CFLAG:LOCAL:ACTIVE}
;		PRINTFORM %GETPLACENAME(CFLAG:LOCAL:現在位置)%
;		PRINTFORML {CFLAG:LOCAL:子宮内体積}

		
	;	CFLAG:2:好感度=1000
;		SIF !MAXBASE:LOCAL:薬物
;		CONTINUE
;		PRINTFORM %CSTR:LOCAL:フルネーム% 
;		PRINTFORM %GETPLACENAME(CFLAG:LOCAL:現在位置)%
;		PRINTFORML {BASE:LOCAL:薬物}/{MAXBASE:LOCAL:薬物}
;	NEXT
;バグ対策
	SIF CFLAG:MASTER:現在位置 == 場所_ダミー
		CFLAG:MASTER:現在位置 = 場所_あなたの家
	CALL 電話の後処理
	VARSET LOCAL
	TFLAG:100 = TFLAG:101
	SIF TFLAG:101 == 1
		TFLAG:100 = 0
	SIF TFLAG:101
		TFLAG:101 --
	SIF TFLAG:COMABLE管理 == 2
		CALL SHOW_TOUCH
	
	;-------------------------------------------------
	;料理の時間経過
	;-------------------------------------------------
	SIF FLAG:料理 > 0
		CALL CEACK_COOKING
	;-------------------------------------------------
	;キャラの態度
	;-------------------------------------------------
	CALL CHARA_ATTITUDE
	;-------------------------------------------------
	;時間経過、キャラの移動
	;-------------------------------------------------
	;行動回数の算出(平均して20分に一回になるようにする)
		行動回数=((DATETIME()-TFLAG:300)-((DATETIME()-TFLAG:300)%20))/20
	SIF ((RAND:20)<((DATETIME()-TFLAG:300)%20))
		行動回数+=1
	IF TFLAG:195
		;TFLAGを戻す
		TFLAG:195 = 0
	ELSEIF 行動回数
				
		REPEAT 行動回数
		CALL CHARA_MOVEMENT
		IF TCVAR:MASTER:130 >= 1
			LOCAL:1 = IN_ROOM("MIN", CFLAG:MASTER:現在位置, "CFLAG", GETNUM(CFLAG, "馴れ合い強度度"), 1)
			LOCAL:2 = CFLAG:(LOCAL:1):6
			IF LOCAL:2 <= 0 && LOCAL:1 > 0 && CFLAG:(LOCAL:1):現在位置 != CFLAG:(LOCAL:1):前ターン位置 && !CFLAG:(LOCAL:1):睡眠
				PRINTFORMW %CALLNAME:(LOCAL:1)%が入ってきたことにより空気が変わってしまった…
				FOR LOCAL, 0, CHARA_ACTIVE
					SIF CFLAG:LOCAL:同室 && CFLAG:LOCAL:馴れ合い強度度 >= 1 && !CFLAG:LOCAL:睡眠
						CALL KOJO_EVENT(8, LOCAL, LOCAL:1)
				NEXT
			ENDIF
		ENDIF
		VARSET LOCAL
		CALL INHABITANT_DO
		CALL VISITER_DO
		REND
	ENDIF
	TFLAG:300 = DATETIME()
	CFLAG:MASTER:睡眠 = 0
	;調教ターゲット＆馴れ合い強度設定
	;CALL INFO_SetTarget
	;MASTERと同室フラグ
	FOR LOCAL,1,CHARA_ACTIVE
		;アイドルはかならずファン一人以上
		SIF TALENT:LOCAL:アイドル && CFLAG:LOCAL:ファン人数 < 1
			CFLAG:LOCAL:ファン人数 = 1
		IF CFLAG:LOCAL:現在位置 == CFLAG:MASTER:現在位置
			;下半身下着が見える状態ならフラグ取得
			SIF GETBIT(FLAG:雑多設定,1) && !EQUIP:LOCAL:下半身上着１ && !EQUIP:LOCAL:下半身上着２ && !EQUIP:LOCAL:スカート && !EQUIP:LOCAL:ボディースーツ && !EQUIP:LOCAL:ワンピース && !EQUIP:LOCAL:着物
				CFLAG:LOCAL:ぱんつ確認 = 1
			;同室じゃなく(遭遇発生時)、睡眠状態でなければ発生
			IF !CFLAG:LOCAL:同室 && !CFLAG:LOCAL:睡眠
				;外出宣言判定
				CALL GoOut_SeeYou(LOCAL)
				IF RESULT:0 > 0
					;出かけたので処理中断
					CONTINUE
				ENDIF
				;遭遇口上と情事発覚口上
				CALL AFFAIR_DISCLOSURE(LOCAL, 0)
				;恋慕じゃない場合に合鍵を回収される
				CALL INFO_CheckMyRoom(LOCAL)
				CFLAG:LOCAL:同室 = 1
			ENDIF
		ELSE
			;相手からこちらが見えていて、相手かこちらかが移動したかこちらが押し倒した
			;SIF CAN_MOVE(CFLAG:LOCAL:現在位置, CFLAG:MASTER:現在位置) == 2 && (CFLAG:MASTER:前ターン位置 != CFLAG:MASTER:現在位置 || CFLAG:LOCAL:前ターン位置 != CFLAG:LOCAL:現在位置 || TRAINNAME:PREVCOM == "押し倒す") && !CFLAG:LOCAL:睡眠
			SIF CAN_MOVE(CFLAG:LOCAL:現在位置, CFLAG:MASTER:現在位置) == 2 && (CFLAG:MASTER:前ターン位置 != CFLAG:MASTER:現在位置 || CFLAG:LOCAL:前ターン位置 != CFLAG:LOCAL:現在位置 || PREVCOM == 350) && !CFLAG:LOCAL:睡眠
				CALL AFFAIR_DISCLOSURE(LOCAL, 1)
			CFLAG:LOCAL:同室 = 0
		ENDIF
	NEXT
	;調教ターゲット＆馴れ合い強度設定
	CALL INFO_SetTarget
	;ウフフ終了TEQUIP処理
	FOR LOCAL,0,CHARA_ACTIVE
		IF !CFLAG:LOCAL:うふふ || CFLAG:LOCAL:現在位置 != CFLAG:LOCAL:前ターン位置
			CALL CLOTHES_RESET(LOCAL)
			CALL CLOTHES_SETTING_TRAIN(LOCAL)
			FOR LOCAL:1,10,150
				TEQUIP:LOCAL:(LOCAL:1) = 0
			NEXT
		;	IF TFLAG:COMABLE管理 < 2
		;		BASE:LOCAL:射精 = 0
		;		TCVAR:LOCAL:イきそう = 0
		;	ENDIF
		ENDIF
		IF BATHROOM(CFLAG:LOCAL:現在位置)
			EQUIP:LOCAL:0 = 0
			IF CFLAG:LOCAL:うふふ || TCVAR:LOCAL:130 > 2
				TEQUIP:(LOCAL):お風呂場プレイ = 1
			ELSE
				TEQUIP:(LOCAL):お風呂場プレイ = 0
			ENDIF
			CALL CLOTHES_SETTING_TRAIN(LOCAL)
		ELSE
			TEQUIP:(LOCAL):お風呂場プレイ = 0
		ENDIF
		IF OPENPLACE(CFLAG:LOCAL:現在位置)
			IF CFLAG:LOCAL:うふふ == 1
				TEQUIP:(LOCAL):野外プレイ = OPENPLACE(CFLAG:LOCAL:現在位置)
			ELSE
				TEQUIP:(LOCAL):野外プレイ = 0
			ENDIF
		ELSE
			TEQUIP:(LOCAL):野外プレイ = 0
		ENDIF
	NEXT
	;-------------------------------------------------
	;情報表示
	;-------------------------------------------------
	DRAWLINE
	;現在時刻と睡眠可否の表示
	CALL INFO_PrintCurrentTime(MASTER)
	;現在位置と汚れ状態の表示
	CALL INFO_PrintCurrentPosition(MASTER)
	;-------------------------------------------------
	;TARGET情報とモード切り替え
	;-------------------------------------------------
	CALL INFO_SetTrainMode
	;特殊プレイ状況を表示する
	CALL SHOW_EQUIP_2
	;HP、MP、勃起、精力のバーを表示する
	CALL INFO_PrintHPMPBar(TARGET,MASTER)
	;ターゲットがいるならムードと理性の表示
	IF TARGET > 0
		CALL INFO_PrintTargetCloth(TARGET)
		SIF  GETBIT(FLAG:雑多設定,7) && !CFLAG:TARGET:名前フラグ && !TALENT:TARGET:ＥＮＥＭＹ
			CALL INFO_PrintTargetTalent(TARGET)
		CALL INFO_PrintTargetMood(TARGET)
		CALL INFO_PrintTargetReason(TARGET)
		SELECTCASE GETBIT(FLAG:雑多設定,11)
			CASE 0
				DRAWLINE
				PRINTFORML [-10] - %CALLNAME:TARGET%の状態を見る
			CASE 1
				DRAWLINE
				CALL INFO_PrintTargetFeeling(TARGET)
				CALL INFO_PrintPalams(TARGET)
				DRAWLINE
				PRINTFORML [-10] - %CALLNAME:TARGET%の状態を閉じる
		ENDSELECT
	ENDIF
	DRAWLINE
	;●マスター状態呼び出し
	SELECTCASE GETBIT(FLAG:雑多設定,12)
		CASE 0
			PRINTFORML [-20] - %CALLNAME:MASTER%の状態を見る
			DRAWLINE
		CASE 1
			PRINTFORML %CALLNAME:MASTER+"の状態："+SHOW_EQUIP_3(MASTER)%
			CALL INFO_PrintPalams(MASTER)
			IF TFLAG:COMABLE管理 == 2 || TFLAG:COMABLE管理 == 3
				;うふふ中
				DRAWLINE
				CALL SHOW_EQUIP_1
			ENDIF
			DRAWLINE
			IF TFLAG:COMABLE管理 == 3 || TFLAG:COMABLE管理 == 4
				;カウンター
				;;別系統を呼び出すようにしたら、いろいろ楽かと思って
				IF TALENT:PLAYER:肉便器
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
;						CALL EVENT_WC_COUNTER(TARGET:LOCAL,LOCAL)
;						CALL EVENT_WC_COUNTER_POSE(TARGET:LOCAL,LOCAL)
					NEXT
;					CALL EVENT_WC_COUNTER_COMBINATION
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
;						CALL EVENT_WC_COUNTER_SOURCE(TARGET:LOCAL,LOCAL)
					NEXT
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
						IF TCVAR:(TARGET:LOCAL):20
;							CALL KOJO_MESSAGE_WC_COUNTER(TARGET:LOCAL)
							DRAWLINE
						ENDIF
					NEXT
					FOR LOCAL,1,CHARA_ACTIVE
						TRYCALL PREGNACY_S_EVENT(LOCAL)
					NEXT
				ELSE
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
						CALL EVENT_COUNTER(TARGET:LOCAL,LOCAL)
						CALL EVENT_COUNTER_POSE(TARGET:LOCAL,LOCAL)
					NEXT
					CALL EVENT_COUNTER_COMBINATION
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
						CALL EVENT_COUNTER_SOURCE(TARGET:LOCAL,LOCAL)
					NEXT
					FOR LOCAL,1,CHARA_ACTIVE
						SIF TARGET:LOCAL <= 0
							CONTINUE
						IF TCVAR:(TARGET:LOCAL):20
							CALL KOJO_MESSAGE_COUNTER(TARGET:LOCAL)
							DRAWLINE
						ENDIF
					NEXT
					FOR LOCAL,1,CHARA_ACTIVE
						TRYCALL PREGNACY_S_EVENT(LOCAL)
					NEXT
				ENDIF
			ENDIF
			PRINTFORML [-20] - %CALLNAME:MASTER%の状態を閉じる
			DRAWLINE
	ENDSELECT
RETURN 0

